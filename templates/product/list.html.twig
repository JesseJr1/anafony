{% extends 'base.html.twig' %}

{% block title %}Nos Produits{% endblock %}

{% block body %}
<h1>Nos Produits</h1>   

<div class="container">
		{% for product in products %}

			<div class="col">
				<h2>{{ product.name}}</h2>
				<a href=" {{ path('app_product', { id: product.id}) }} " >Voir</a>
			{% endfor %}
		</div>
	</div>

{% endblock %}
